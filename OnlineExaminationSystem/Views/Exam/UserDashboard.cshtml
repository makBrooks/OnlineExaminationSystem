
@{
    ViewData["Title"] = "User Dashboard";
    Layout = "_User";
}

<h1>Welcome User</h1>
<div>
    <table class="table  table-striped">
        <tr>
            <th colspan="2" align="justify">
                <h3>WelCome @ViewBag.userName !!!</h3>
            </th>
        </tr>
    </table>

    <table class="table  table-striped">
        <tr>
            <th colspan="2" align="justify">
                Time Duration:-@ViewBag.Time
            </th>
        </tr>
        <tr>
            <th colspan="2" align="justify">
                No of Questions:-@ViewBag.NoOFQuestion
            </th>
        </tr>
        <tr>
            <th colspan="2" align="justify">
                Rules:-@ViewBag.Rules
            </th>
        </tr>
    </table>
</div>

